<!doctype html>
<html lang="en">
 {{> head}}
  <body>
<div id="loginBg"> 
<div id="regContainer" class="container">
<div class="col-8 offset-2"> 
<img src="/familyHealth-logo.png" id="logo" class="d-inline-block align-top" />
<form action="/register" method="POST">
    <div class="form-group">
      <label id="labels" for="exampleInputEmail1">Family Name</label>
      <input type="text" name="family_name" class="form-control" id="exampleInputFamilyName"  placeholder="Enter Family Name">

    </div>
    <div class="form-group">
      <label id="labels" for="exampleInputPassword1">User Name</label>
      <input type="text" name="username" class="form-control" id="exampleInputUserName" placeholder="Username">
    </div>

      <div class="form-group">
      <label id="labels" for="exampleInputPassword1">Address</label>
      <input type="text" name="address" class="form-control" id="exampleInputAddress" placeholder="Address">
    </div>
    

      <div class="form-group">
      <label id="labels" for="exampleInputPassword1">Password</label>
      <input type="password" name="password" class="form-control" id="exampleInputPassword" placeholder="Password">
      </div>

    <button id="buttons" type="submit" class="btn btn-primary">Register</button>
    <br>
    <a id="question" href="/login"> Already a member? Login </a>
  </form>
</div>
</div>
</body>
{{> footer}}
</html>